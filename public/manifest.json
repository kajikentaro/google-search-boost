{
  "manifest_version": 3,
  "name": "Google Search Boost (vim-like keyboard shortcuts for search page)",
  "version": "0.0.1",
  "description": "Boost your googling with vim-like shortcuts",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/search?*"],
      "js": ["main.js"],
      "css": ["style.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["arrow.png"],
      "matches": ["https://www.google.com/*"]
    }
  ]
}
